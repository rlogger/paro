// Xcode Configuration for Eater iOS App

// MARK: - Deployment
IPHONEOS_DEPLOYMENT_TARGET = 17.0
TARGETED_DEVICE_FAMILY = 1,2
SUPPORTS_MACCATALYST = NO
SUPPORTS_MAC_DESIGNED_FOR_IPHONE_IPAD = NO

// MARK: - SDK
SDKROOT = iphoneos

// MARK: - Build Settings
SWIFT_VERSION = 5.9
SWIFT_OPTIMIZATION_LEVEL = -Onone
ENABLE_BITCODE = NO

// MARK: - Code Signing
CODE_SIGN_STYLE = Automatic
DEVELOPMENT_TEAM =

// MARK: - Product
PRODUCT_NAME = Eater
PRODUCT_BUNDLE_IDENTIFIER = com.eater.app

// MARK: - Swift Flags
SWIFT_ACTIVE_COMPILATION_CONDITIONS = DEBUG
OTHER_SWIFT_FLAGS = -D DEBUG
